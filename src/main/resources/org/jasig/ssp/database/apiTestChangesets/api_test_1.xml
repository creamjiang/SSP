<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">

    <changeSet id="Load API Integration Test Data Postgre" author="paul.spaude">
        <preConditions onFail="MARK_RAN" onFailMessage="Skipping Postgres API Integration Data Load">
            <dbms type="postgresql" />
        </preConditions>
        <sqlFile path="sspIntegrationAPITestDataPSQL.sql" relativeToChangelogFile="true" splitStatements="false" stripComments="false"></sqlFile>
	</changeSet>

    <changeSet id="Load API Integration Test Data MSSQL" author="paul.spaude">
        <preConditions onFail="MARK_RAN" onFailMessage="Skipping MSSQL API Integration Data Load">
            <dbms type="mssql" />
        </preConditions>
        <sqlFile path="sspIntegrationAPITestDataMSSQL.sql" relativeToChangelogFile="true" splitStatements="false" stripComments="false"></sqlFile>
    </changeSet>

</databaseChangeLog>